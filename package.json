{
  "name": "ts-rest",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "version-packages": "changeset version",
    "version-packages-update-lockfile": "changeset version && pnpm install --no-frozen-lockfile",
    "release": "pnpm nx run-many --target=build && changeset publish",
    "nest": "pnpm nx run example-nest:serve",
    "affected:all": "pnpm nx affected:lint && pnpm nx affected:test && pnpm nx affected:build",
    "docs:build": "pnpm nx run docs:build && ts-node ./tools/scripts/docs-redirects.ts",
    "docs:start": "nx serve docs",
    "example-microservice:serve": "pnpm nx run-many --target=serve --projects='example-microservice*' --parallel=10",
    "ts-rest:build-watch": "pnpm nx run-many --target=build --projects=ts-rest-react-query --parallel --watch",
    "ci:nx-affected": "yarn ts-node ./tools/workflows/nx-affected.ts",
    "build-test-all": "nx run-many --targets=lint,test,build",
    "nx": "nx"
  },
  "engines": {
    "pnpm": "8",
    "node": "^18 || ^20"
  },
  "packageManager": "pnpm@8.15.7+sha256.50783dd0fa303852de2dd1557cd4b9f07cb5b018154a6e76d0f40635d6cee019",
  "repository": "https://github.com/ts-rest/ts-rest",
  "private": true,
  "workspaces": [
    "libs/ts-rest/*"
  ],
  "dependencies": {
    "@tanstack/query-core": "^5.51.9",
    "@tanstack/react-query": "^5.51.9",
    "@ts-rest/core": "3.45.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@nx/eslint": "17.1.1",
    "@nx/eslint-plugin": "17.1.1",
    "@nx/jest": "17.1.1",
    "@nx/rollup": "17.1.1",
    "@nx/workspace": "17.1.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "29.4.4",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "commander": "^10.0.1",
    "eslint": "8.53.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jsdom": "22.1.0",
    "nx": "17.1.1",
    "prettier": "^3.0.3",
    "rollup": "^3.10.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "ts-jest": "29.1.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.11.1",
    "typescript": "5.2.2"
  }
}
